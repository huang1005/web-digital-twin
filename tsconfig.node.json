{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "composite": true,
    "module": "ES2022",
    "tsBuildInfoFile": "./node_modules/.rs_cache",
    "moduleResolution": "bundler",
    "target": "ES2017",
    "types": [
      "node"
    ],
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@root/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/vite.config.ts",
    "config"
  ]
}